{
  "LiveMetrics": {
    "ServiceBusMetricsOptions": {
      "enabled": true,
      "IntervalInSeconds": 10,
      "Accounts": [
        {
          "ResourceGroup": "anbossar-py-test",
          "Namespace": "anpyservicebus",
          "Queues": [
            "*put"
          ],
          "Topics": [
            "topic-1",
            "*topic"
          ]
        }
      ]
    },
    "StorageAccountMetricsOptions": {
      "enabled": false,
      "intervalInSeconds": 10,
      "cutoffThreshold": 10000,
      "Accounts": [
        {
          "ConnectionString": "",
          "Containers": [
            "007-data-validated"
          ]
        }
      ]
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "WithThreadId"
    ],
    "WriteTo": [
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
